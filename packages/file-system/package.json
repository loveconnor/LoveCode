{
    "name": "@fluxly/file-system",
    "description": "Browser-based file system abstraction for Fluxly",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "main": "src/index.ts",
    "module": "src/index.ts",
    "types": "src/index.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/fluxly-dev/fluxly.git"
    },
    "scripts": {
        "clean": "rm -rf node_modules",
        "lint": "eslint . --max-warnings 0",
        "format": "eslint --fix .",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "Fluxly",
        "file-system",
        "zenfs",
        "browser-fs"
    ],
    "author": {
        "name": "Fluxly",
        "email": "contact@fluxly.com"
