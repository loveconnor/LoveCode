        "./hooks": "./src/hooks/index.ts"
    },
    "devDependencies": {
        "@fluxly/eslint": "*",
        "@fluxly/typescript": "*",
        "@types/lodash.debounce": "^4.0.9",
        "@types/react": "^18.3.1",
        "eslint": "^9.0.0",
        "react": "^18.3.1",
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "@zenfs/core": "2.4.0",
        "@zenfs/dom": "1.2.5",
        "lodash.debounce": "^4.0.8"
