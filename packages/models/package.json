{
    "name": "@fluxly/models",
    "description": "Common models shared between Fluxly packages",
    "version": "0.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/fluxly-dev/fluxly.git"
    },
    "type": "module",
    "module": "src/index.ts",
    "main": "dist/index.js",
    "scripts": {
        "build": "bun build src/index.ts --outfile=dist/index.js",
        "clean": "rm -rf node_modules",
        "lint": "eslint . --max-warnings 0",
        "format": "eslint --fix .",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "Fluxly",
        "models"
    ],
    "author": {
        "name": "Fluxly",
        "email": "contact@fluxly.com"
    },
    "license": "Apache-2.0",
    "homepage": "https://fluxly.com",
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*/index.ts"
    },
    "devDependencies": {
        "@fluxly/eslint": "*",
        "@fluxly/typescript": "*",
        "ai": "5.0.60",
        "eslint": "^9.0.0",
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "zod": "^4.1.3"
    }
}